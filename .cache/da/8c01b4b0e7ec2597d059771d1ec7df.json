ize: .80rem;\n  list-style: none;\n  display: flex;\n  margin-top: 1em;\n\n  .link-nav-items {\n    position: relative;\n    text-decoration: none;\n    color: #9b9b9b;\n    margin-right: 2em;\n    transition: color .5s ease-in-out;\n\n    &::before {\n      content: \"\";\n      position: absolute;\n      left: 0;\n      bottom: -1%;\n      background-color: #9b9b9b;\n      width: 100%;\n      height: 2px;\n      transform: scaleX(0);\n      transform-origin: left;\n      transition: transform 650ms;\n    }\n  \n    &:hover::before {\n      transform: scaleX(1);\n      background-color: #000;\n    }\n\n    &:hover {\n      color: #000;\n    }\n  }\n}\n\n.scroll-down {\n  position: absolute;\n  left: 11%;\n  bottom: 0;\n  transform: translateY(-100%);\n  text-decoration: none;\n  font-size: .80rem;\n  color: #9b9b9b;\n  cursor: pointer;\n  width: 80%;\n  transition: all 0.5s ease-in-out;\n\n  &:hover {\n    color: #000;\n  }\n}\n\n// project\n.project {\n  margin-bottom: 8em;\n\n  .project-grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(15rem, 1fr));\n    grid-gap: 1.5em;\n    overflow: hidden;\n    justify-content: space-around;\n  }\n\n  .project-grid-item img{\n    .slide-in {\n      opacity: 0;\n      transition: all .5s;\n    }\n  \n    .align-left.slide-in {\n      transform: translateX(-30%) scale(0.95);\n    }\n  \n    .slide-in.active {\n      opacity: 1;\n      transform: translateX(0%) scale(1);\n    }\n  }\n\n  .project-featured {\n    cursor: pointer;\n    margin-bottom: 5em;\n  }\n\n  .project-featured img,\n  .project-grid-item img {\n    max-width: 100%;\n  }\n\n  .project-featured > .project-title,\n  .project-grid-item > .project-title {\n    font-size: .95rem;\n    font-weight: 500;\n  }\n\n  .project-featured > .project-background,\n  .project-grid-item > .project-background {\n    font-size: .80rem;\n    color: #9b9b9b;\n    line-height: 1rem;\n  }\n  \n}\n\n// footer \nfooter {\n  background-color: #181818;\n  height: 40vh;\n  color: #fff;\n  // display: flex;\n  // justify-content: space-between;\n  // position: relative;\n\n  a {\n    font-size: 1rem;\n    text-decoration: none;\n    color: #fff;\n    position: relative;\n    left: 5%;\n    top: 50%;\n\n    &::before {\n      content: \"\";\n      position: absolute;\n      left: 0;\n      bottom: -.5rem;\n      background: linear-gradient(to left, #9b9b9b, #9b9b9b);\n      width: 100%;\n      height: 2px;\n      transition: all 650ms;\n    }\n\n    &:hover::before {\n      background: linear-gradient(to left, #fff, #fff);\n    }\n  }\n\n  // .up-arrow {\n  //   max-width: 100%;\n  //   position: relative;\n  //   top: 70%;\n  //   // right: -7em;\n  //   right: -4em;\n  //   font-size: 1.4rem;\n  //   cursor: pointer;\n  // }\n}\n\n@media screen and (min-width: 380px) {\n  .headline {\n    .name {\n      font-size: 1.40rem;\n    }\n\n    .tagline {\n      font-size: 1.3rem;\n    }\n  }\n\n  .link-nav {\n    font-size: .90rem;\n  }  \n\n  .scroll-down {\n    font-size: .85rem;\n  }\n\n  .project-featured > .project-title,\n  .project-grid-item > .project-title {\n    font-size: 1.1rem;\n  }\n\n  a {\n    font-size: 1.1rem;\n  }\n}\n\n@media screen and (min-width: 770px) {\n  .project-featured img,\n  .project-grid-item img {\n    filter: grayscale(100%);\n    transition: all 0.5s ease-in-out;\n\n    &:hover {\n      filter: grayscale(0%);\n    }\n  }\n\n  .project-grid-item {\n    cursor: pointer;\n  }\n}"},"lineCount":null}},"error":null,"hash":"6f602f497fae5b5b84634d9fcbaf7588","cacheData":{"env":{}}}